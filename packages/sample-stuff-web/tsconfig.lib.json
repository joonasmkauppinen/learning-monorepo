{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "../../dist/sample-stuff-web",
    "declaration": true,
    "types": [],
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "jsx": "react"
  },
  "exclude": ["**/*.spec.ts", "**/*.spec.tsx"],
  "include": ["**/*.js", "**/*.jsx", "**/*.ts", "**/*.tsx"]
}
